{
  "name": "ngx-webstorage-service",
  "version": "5.0.0",
  "description": "Angular service wrappers for the webstorage API",
  "homepage": "https://github.com/dscheerens/ngx-webstorage-service",
  "author": "Daan Scheerens",
  "license": "MIT",
  "keywords": [
    "angular",
    "ng",
    "ngx",
    "webstorage",
    "storage",
    "localstorage",
    "sessionstorage",
    "local",
    "session"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/dscheerens/ngx-webstorage-service.git"
  },
  "scripts": {
    "ng": "ng",
    "lint": "ng lint --project=lib",
    "test": "ng test --project=lib --watch=false --code-coverage",
    "test.watch": "ng test --project=lib --watch=true --code-coverage --browsers=Chrome",
    "build": "ng build --project=lib --configuration production",
    "release": "standard-version --message \"release: v%s\"",
    "publish.dist": "npm publish build/dist"
  },
  "peerDependencies": {
    "@angular/core": ">=13.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.3.8",
    "@angular-eslint/builder": "^13.5.0",
    "@angular-eslint/eslint-plugin": "^13.5.0",
    "@angular-eslint/eslint-plugin-template": "^13.5.0",
    "@angular-eslint/template-parser": "^13.5.0",
    "@angular/cli": "^13.3.8",
    "@angular/common": "^13.3.11",
    "@angular/compiler": "^13.3.11",
    "@angular/compiler-cli": "^13.3.11",
    "@angular/core": "^13.3.11",
    "@angular/platform-browser": "^13.3.11",
    "@angular/platform-browser-dynamic": "^13.3.11",
    "@dscheerens/eslint-config-angular": "1.0.0",
    "@types/jasmine": "~3.10.3",
    "@types/jasminewd2": "~2.0.10",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "^8.19.0",
    "jasmine-core": "^3.10.1",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^4.0.1",
    "ng-packagr": "^13.3.1",
    "rxjs": "^7.5.4",
    "standard-version": "^9.5.0",
    "tslib": "^2.3.1",
    "typescript": "~4.6.4",
    "zone.js": "~0.11.4"
  }
